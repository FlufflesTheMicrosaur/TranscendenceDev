<!-- EI200-CLASS FREIGHTER -->

	<!-- Level 4: EI200-class Freighter -->

	<ShipClass UNID="&HS_scEI200PlayerL4;"
			manufacturer=		"Earth Industries"
			class=				"EI200"
			type=				"freighter"

			level=				"4"
			attributes=			"commonwealth, corporate, EI, playerClass, shipBroker"
			inherit=			"&HS_scEI200Stock;"
			>

		<!-- Configuration -->
		
		<Armor
			armorID=			"&HS_itAdvancedReactiveArmor;"
			count=				"6"
			/>
		
		<Devices>
			<Table>
				<Group chance="40">
					<Device deviceID="&HS_itOmniTurbolaserCannon;"/>
					<Device deviceID="&HS_itMAGLauncher;">
						<Items>
							<Item			  count="50-100" item="&HS_it600HexageneMAG;"/>
							<Item chance="40" count="50-100" item="&HS_it800HexageneMAG;"/>
						</Items>
					</Device>
					<Device chance="50" deviceID="&HS_itLaserEnhancer;"/>
				</Group>
				<Group chance="40">
					<Device deviceID="&HS_itOmniPartisanCannon;"/>
					<Device deviceID="&HS_itFlenserCannon;"/>
					<Device deviceID="&HS_itKineticEnhancer;"/>
				</Group>
				<Group chance="20">
					<Device deviceID="&HS_itOmniTurbolaserCannon;"/>
					<Device deviceID="&HS_itSlamCannon;"/>
					<Device deviceID="&HS_itLaserEnhancer;"/>
				</Group>
			</Table>

			<Table>
				<Device chance="40" deviceID="&HS_itClass3Deflector;"/>
				<Device chance="30" deviceID="&HS_itCeratopsQuasiArmor1;"/>
				<Device chance="30" deviceID="&HS_itR1Deflector;"/>
			</Table>

			<Device deviceID="&HS_itCargoHoldExpansion;"/>
			<Device deviceID="&HS_it100MWReactor;"/>
		</Devices>

		<!-- Player Settings -->
		
		<PlayerSettings
			desc=				"The EI200 has more than double the cargo capacity of the EI500."
			/>
		
	</ShipClass>

