<?xml version="1.0" encoding="utf-8"?>

<TranscendenceModule>

<!-- TABLES -->

	<SystemPartTable unid="&GLXY_tbPrimordialPlanets;">
		<PrimordialPrimary>
			<LocationCriteriaTable>
				<Table criteria="*innerSystem">
					<Lookup chance="70" table="PrimordialPlanetoidPrimary"/>
					<Lookup chance="30" table="PrimordialTerrestrialPrimary"/>
				</Table>
				<Table criteria="*lifeZone">
					<Lookup chance="40" table="PrimordialPlanetoidPrimary"/>
					<Lookup chance="60" table="PrimordialTerrestrialPrimary"/>
				</Table>
				<Table criteria="*outerSystem">
					<Lookup chance="80" table="PrimordialPlanetoidPrimary"/>
					<Lookup chance="20" table="PrimordialTerrestrialPrimary"/>
				</Table>
			</LocationCriteriaTable>
		</PrimordialPrimary>

		<PrimordialPlanetoidPrimary>
			<Lookup table="StandardPlanetoidMorphology">
				<_PlanetType>
					<Table>
						<Station chance="60" type="&GLXY_stPrimordialPlanetoidSizeF;" showOrbit="true"/>
						<Station chance="40" type="&GLXY_stPrimordialPlanetoidSizeF2;" showOrbit="true"/>
					</Table>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="planet, planetary, planetoid, primordial"/>
				</_PlanetLabel>
			</Lookup>
		</PrimordialPlanetoidPrimary>

		<PrimordialTerrestrialPrimary>
			<Lookup table="StandardTerrestrialMorphology">
				<_PlanetType>
					<Table>
						<Station chance="30" type="&GLXY_stPrimordialTerrestrialSizeH;" showOrbit="true"/>
						<Station chance="50" type="&GLXY_stPrimordialTerrestrialSizeH2;" showOrbit="true"/>
						<Station chance="20" type="&GLXY_stPrimordialTerrestrialSizeJ;" showOrbit="true"/>
					</Table>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="planet, planetary, primordial, terrestrial"/>
				</_PlanetLabel>
			</Lookup>
		</PrimordialTerrestrialPrimary>

		<PrimordialGasGiantPrimary>
			<Lookup table="SmallGasGiantMorphology">
				<_PlanetType>
					<Table>
						<Station chance="100" type="&GLXY_stPrimordialTerrestrialSizeH;" showOrbit="true"/>
					</Table>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="gasGiant, planet, planetary, primordial"/>
				</_PlanetLabel>
			</Lookup>
		</PrimordialGasGiantPrimary>
	</SystemPartTable>

<!-- ASTEROIDS & PLANETS -->

	<!-- Size A Primordial Asteroid -->

	<StationType UNID="&GLXY_stPrimordialAsteroidSizeA;"
		name=				"primordial asteroid"
		attributes=			"asteroid, generic, inner, lifeZone, outer, primordial"
		inherit=			"&GLXY_baPrimordialAsteroid;"

		scale=				"world"
		size=				"6"
		anonymous=			"true"

		ejectaType=			"&GLXY_vtStoneAsteroidEjecta;"
		>
		<ImageLookup imageID="&GLXY_rsPrimordialAsteroidSizeA;"/>

		<Items>
			<Table>
				<Null chance="92"/>
				<Lookup chance="8" table="&GLXY_tbAsteroidOre;"/>
