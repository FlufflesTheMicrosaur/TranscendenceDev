		<ImageVariants>
			<Image			imageID="&GLXY_rsWorldsRingedGiant1;" imageX="0" imageY="0" imageWidth="512" imageHeight="256"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant2;" imageX="0" imageY="0" imageWidth="512" imageHeight="256"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant3;" imageX="0" imageY="0" imageWidth="512" imageHeight="340"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant4;" imageX="0" imageY="0" imageWidth="462" imageHeight="400"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant5;" imageX="0" imageY="0" imageWidth="412" imageHeight="400"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant6;" imageX="0" imageY="0" imageWidth="412" imageHeight="340"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant7;" imageX="0" imageY="0" imageWidth="448" imageHeight="340"/>
			<Image			imageID="&GLXY_rsWorldsRingedGiant8;" imageX="0" imageY="0" imageWidth="432" imageHeight="380"/>
		</ImageVariants>
	</StationType>

