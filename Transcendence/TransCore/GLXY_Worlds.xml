<?xml version="1.0" encoding="utf-8"?>

<TranscendenceModule>

<!-- TABLES -->

	<SystemPartTable unid="&GLXY_tbStandardPlanets;">

		<DesertPrimary>
			<Lookup table="StandardTerrestrialMorphology">
				<_PlanetType>
					<Station type="&GLXY_stDesertTerrestrialSizeH;" showOrbit="true"/>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="desert, planet, planetary, terrestrial"/>
				</_PlanetLabel>
			</Lookup>
		</DesertPrimary>

		<GreenhousePrimary>
			<Lookup table="StandardTerrestrialMorphology">
				<_PlanetType>
					<Station type="&GLXY_stGreenhouseTerrestrialSizeI;" showOrbit="true"/>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="greenhouse, planet, planetary, terrestrial"/>
				</_PlanetLabel>
			</Lookup>
		</GreenhousePrimary>

		<GasGiantPrimary>
			<Table>
				<Lookup chance="60" table="MethaneGasGiantPrimary"/>
				<Lookup chance="30" table="HydrogenGasGiantPrimary"/>
				<Lookup chance="10" table="RingedGasGiantPrimary"/>
			</Table>
		</GasGiantPrimary>

		<HydrogenGasGiantPrimary>
			<Lookup table="StandardGasGiantMorphology">
				<_PlanetType>
					<Table>
						<Station chance="100" type="&GLXY_stHydrogenGasGiantSizeM;" showOrbit="true"/>
					</Table>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="gasGiant, hydrogenGiant, planet, planetary"/>
				</_PlanetLabel>
			</Lookup>
		</HydrogenGasGiantPrimary>

		<MethaneGasGiantPrimary>
			<Lookup table="SmallGasGiantMorphology">
				<_PlanetType>
					<Table>
						<Station chance="100" type="&GLXY_stMethaneGasGiantSizeK;" showOrbit="true"/>
					</Table>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="gasGiant, methaneGiant, planet, planetary"/>
				</_PlanetLabel>
			</Lookup>
		</MethaneGasGiantPrimary>

		<RingedGasGiantPrimary>
			<Lookup table="StandardGasGiantMorphology">
				<_PlanetType>
					<Table>
						<Station chance="100" type="&GLXY_stRingedGasGiant;" showOrbit="true"/>
					</Table>
				</_PlanetType>
				<_PlanetLabel>
					<Label attributes="gasGiant, planet, planetary, ringedGiant"/>
				</_PlanetLabel>
			</Lookup>
		</RingedGasGiantPrimary>

	</SystemPartTable>

<!-- ASTEROIDS & PLANETS -->

	<!-- Biosphere -->

	<StationType UNID="&GLXY_stBiosphereTerrestrialSizeI;"
		name=				"(biosphere terrestrial)"
		scale=				"world"
		size=				"10000"

		attributes=			"biosphere, generic, terrestrial"
		>
		<ImageVariants>
			<Image	imageID="&GLXY_rsBiosphereTerrestrials;" imageX="0"   imageY="0"   imageWidth="230" imageHeight="230"/>
			<Image	imageID="&GLXY_rsBiosphereTerrestrials;" imageX="0"   imageY="230" imageWidth="230" imageHeight="230"/>
			<Image	imageID="&GLXY_rsBiosphereTerrestrials;" imageX="0"   imageY="460" imageWidth="230" imageHeight="230"/>
		</ImageVariants>
	</StationType>

	<!-- Desert Planet -->

	<StationType UNID="&GLXY_stDesertTerrestrialSizeH;"
		name=				"(desert terrestrial)"
		scale=				"world"
		size=				"8300"

		attributes=			"desert, generic, terrestrial"
		>
		<ImageVariants>
			<Image	imageID="&GLXY_rsDesertTerrestrials;" imageX="0"   imageY="0"   imageWidth="220" imageHeight="220"/>
			<Image	imageID="&GLXY_rsDesertTerrestrials;" imageX="0"   imageY="220" imageWidth="220" imageHeight="220"/>
			<Image	imageID="&GLXY_rsDesertTerrestrials;" imageX="0"   imageY="440" imageWidth="220" imageHeight="220"/>
		</ImageVariants>
	</StationType>

	<!-- Greenhouse Planet -->

	<StationType UNID="&GLXY_stGreenhouseTerrestrialSizeI;"
		name=				"(greenhouse terrestrial)"
		scale=				"world"
		size=				"13250"

		attributes=			"generic, greenhouse, terrestrial"
		>
		<ImageVariants>
			<Image	imageID="&GLXY_rsGreenhouseTerrestrials;" imageX="0"   imageY="0"   imageWidth="250" imageHeight="250"/>
			<Image	imageID="&GLXY_rsGreenhouseTerrestrials;" imageX="0"   imageY="250" imageWidth="250" imageHeight="250"/>
			<Image	imageID="&GLXY_rsGreenhouseTerrestrials;" imageX="0"   imageY="500" imageWidth="250" imageHeight="250"/>
		</ImageVariants>
	</StationType>

	<!-- Hydrogen Gas Giant -->

	<StationType UNID="&GLXY_stHydrogenGasGiantSizeM;"
		name=				"(hydrogen gas giant)"
		scale=				"world"
		size=				"151500"

		attributes=			"gasGiant, generic"
		>
		<ImageVariants>
			<Image	imageID="&GLXY_rsHydrogenGasGiant01;" imageX="0"   imageY="0"   imageWidth="450" imageHeight="450"/>
			<Image	imageID="&GLXY_rsHydrogenGasGiant02;" imageX="0"   imageY="0"   imageWidth="450" imageHeight="450"/>
			<Image	imageID="&GLXY_rsHydrogenGasGiant03;" imageX="0"   imageY="0"   imageWidth="450" imageHeight="450"/>
		</ImageVariants>
	</StationType>

	<!-- Methane Gas Giant -->

	<StationType UNID="&GLXY_stMethaneGasGiantSizeK;"
		name=				"(methane gas giant)"
		scale=				"world"
		size=				"49500"

		attributes=			"gasGiant, generic"
		>
		<ImageVariants>
			<Image	imageID="&GLXY_rsMethaneGasGiant01;" imageX="0"   imageY="0"   imageWidth="340" imageHeight="340"/>
		</ImageVariants>
	</StationType>

	<!-- Ringed Gas Giant
	
